{
  "name": "react-raster",
  "version": "5.2.0",
  "description": "Advanced Grid-System based on Styled Components, which is highly customizable and ready for server-side-rendering.",
  "author": "AndreasFaust",
  "license": "MIT",
  "repository": "AndreasFaust/react-raster",
  "homepage": "https://andreasfaust.github.io/react-raster/",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "prop-types": "^15.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-react": "^7.12.7",
    "@typescript-eslint/eslint-plugin": "4.8.2",
    "@typescript-eslint/parser": "4.8.2",
    "babel-eslint": "10.1.0",
    "babel-plugin-macros": "^3.0.0",
    "eslint": "7.14.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "5.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.2.0",
    "gatsby": "^2.27.4",
    "gatsby-plugin-layout": "^1.5.0",
    "gatsby-plugin-react-helmet": "^3.5.0",
    "gatsby-plugin-sass": "^2.6.0",
    "gatsby-plugin-typescript": "^2.7.0",
    "generate-changelog": "^1.8.0",
    "gh-pages": "^3.1.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-select": "^3.1.1",
    "rollup": "^2.33.3",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-url": "^3.0.1",
    "sass": "^1.29.0",
    "styled-components": "^5.2.1"
  },
  "peerDependencies": {
    "react": "^16.8.5",
    "react-dom": "^16.8.5",
    "styled-components": "^4.4.0"
  },
  "keywords": [
    "React",
    "Grid",
    "Layout",
    "Styled Components",
    "Server Side Rendering",
    "Raster"
  ],
  "scripts": {
    "dev": "gatsby develop",
    "start": "yarn dev",
    "build": "rollup -c",
    "release:major": "yarn build && git add . && changelog -M && git add CHANGELOG.md && git commit -m 'updated CHANGELOG.md' && npm version major && git push origin && git push origin --tags && npm publish",
    "release:minor": "yarn build && git add . && changelog -m && git add CHANGELOG.md && git commit -m 'updated CHANGELOG.md' && npm version minor && git push origin && git push origin --tags && npm publish",
    "release:patch": "yarn build && git add . && changelog -p && git add CHANGELOG.md && git commit -m 'updated CHANGELOG.md' && npm version patch && git push origin && git push origin --tags && npm publish",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public"
  }
}
